#include <iostream>
using namespace std;
class person
{
public:
    person()
    {
        cout << "无参构造函数的调用\n";
    }
    person(int a)
    {
        cout << "有参构造函数的调用\n";
    }
    person(const person& p)
    {
        cout << "拷贝函数的调用\n";
    }
    ~person()
    {
        cout << "析构函数的调用\n";
    }
};

int main()
{
    person p0;
    person p1(10);
    person p2 = p1;
    person p3(p2);
}
