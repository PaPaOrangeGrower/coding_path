#include <iostream>
using namespace std;
int main()
{
	int arr[10];
	scanf_s("%d%d%d%d%d%d%d%d%d%d:\n",  &arr[0], &arr[1],&arr[2],&arr[3],&arr[4],&arr[5], &arr[6], &arr[7], &arr[8], &arr[9]);
	int high;
	scanf_s("%d:\n", &high);
	register int i = 0;
	int sum=0;
	for (; i < 10; i++)
	{
		if (arr[i] <= high + 30)
			sum += 1;
	}
	printf("%d", sum);
	return 0;
}
